
public abstract class Animal {
	String raca;
	String nome;
	int idade;
	int qtdPatas;
	float peso;
	
	void comer() {
		System.out.println("Comendo.....");
	}
	
	void mover() {
		System.out.println("Movendo.....");
	}
	
	void comunicar() {
		System.out.println("Comunicando.....");
	}
	
	void dormir() {
		System.out.println("Dormindo.....");
	}
	
	
	
	
	
	

}
